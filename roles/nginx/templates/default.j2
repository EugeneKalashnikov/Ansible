user  root;
worker_processes  1;
error_log  error.log;
pid        nginx.pid;
worker_rlimit_nofile 8192;

events {
  worker_connections  4096;  ## Default: 1024
}

http {
    upstream web-app {
           server {{ web_ip1 }};
           server {{ web_ip2 }};
           server {{ web_ip3 }};
    }

    server {
        listen 80;

        location / {
            proxy_pass http://web-app:5000;
        }
    }
}
